{
    "name": "tumedia/aether",
    "description": "Aether - A modular PHP framework",
    "keywords": ["aether"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "filp/whoops": "^2.1",
        "illuminate/config": "5.5.*",
        "sentry/sentry": "1.8.*",
        "smarty/smarty": "3.1.30",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Aether\\": "./src"
        },
        "files": [
            "./src/class_aliases.php",
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./tests"
        }
    },
    "bin": [
        "./bin/generateConfig",
        "./bin/updateLocales"
    ],
    "config": {
        "sort-packages": true
    }
}
