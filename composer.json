{
    "name": "tumedia/aether",
    "description": "Aether - A modular PHP framework",
    "keywords": ["aether"],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "smarty/smarty": "3.1.30",
        "vlucas/phpdotenv": "^2.4",
        "sentry/sentry": "1.5.*",
        "illuminate/config": "5.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "classmap": [
            "./src"
        ],
        "files": ["./src/helpers.php"]
    },
    "autoload-dev": {
        "classmap": [
            "./tests"
        ]
    },
    "bin": [
        "./bin/generateConfig",
        "./bin/updateLocales"
    ]
}
